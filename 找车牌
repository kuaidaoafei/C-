#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
    int n,i,cnt=0;
    char c[10];
    cin>>n;
    for(i=0; i<=n-1; i++)
    {
        cin>>c;
        if((c[0]>='A'&&c[0]<='G')||(c[0]>='R'&&c[0]<='T'))
        {
            cnt++;
        }
    }
    cout<<n-cnt<<endl;
    return 0;
}
